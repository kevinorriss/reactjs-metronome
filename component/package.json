{
  "name": "@kevinorriss/react-metronome",
  "version": "2.0.1",
  "description": "Plays a sound at the specified beats per minute",
  "main": "build/metronome.min.js",
  "scripts": {
    "build": "rollup -c -o build/metronome.min.js",
    "dev": "rollup -c --watch -o build/metronome.min.js",
    "test": "jest --watchAll"
  },
  "jest": {
    "setupFiles": [
      "./test/jest.config.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "Kevin Orriss <kevinorriss@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinorriss/reactjs-metronome"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "jest": "^26.4.2",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.28.2",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "rc-slider": "^8.7.1",
    "react": "^16.14.0"
  }
}
